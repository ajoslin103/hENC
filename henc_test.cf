body common control
{
      inputs => {
		  "enc.cf",
		  "lib/henc_lib.cf",
		  "test/lib.cf"
      } ;

      version => "hENC-v0.0.1" ;

      bundlesequence => { "prepare_henc_test",
			  "henc_cfg",
			  "henc_test" } ;
}

bundle common prepare_henc_test
{
  classes:
      "is_running_henc_tests"
	  comment    => "Signal to henc_cfg we are running a test",
	  expression => "any" ;
}

bundle agent henc_test
{
  vars:
    any::
      "enclist" slist => { "test/henc_test" } ;

  methods:
      "henc"
	  comment => "test ENC",
	  usebundle => henc("henc_test.enclist") ;
}